{
  "name": "ts-starter",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register --transpile-only src/main",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --transpile-only src/main",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix .",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^2.19.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "morgan": "^1.10.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.0.5",
    "prisma": "^2.19.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
